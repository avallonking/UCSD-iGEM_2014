      "class node(object):\n",
      "    \n",
      "    def __init__(self, value):\n",
      "        self.value = value\n",
      "        self.children = []\n",
      "        self.parent = None\n",
      "    \n",
      "    def __repr__(self, level=0):\n",
      "        ret = \"\\t\"*level+repr(self.value)+\"\\n\"\n",
      "        for child in self.children:\n",
      "            ret += child.__repr__(level+1)\n",
      "        return ret\n",
      "    \n",
      "    def append_child(self, obj):\n",
      "        self.children.append(obj)\n",
      "        if type(obj) == node:\n",
      "            ###print \"append_child: obj is instance of node\"\n",
      "            obj.parent = self\n",
      "    \n",
      "    def get_all_leaf(self):\n",
      "        leaf_list = []\n",
      "        if self.children != []:\n",
      "            for child_node in self.children:\n",
      "                leaf_list.append(child_node.get_all_leaf())\n",
      "            ###print \"get_all_leaf: returning leaf_list: before merge \\t\", leaf_list\n",
      "            leaf_list = uniquely_merge_multi_dimentional_list_of_lists(leaf_list)\n",
      "            ###print \"get_all_leaf: returning leaf_list: after merge \\t\", leaf_list\n",
      "            return leaf_list\n",
      "        ###print \"get_all_leaf: no children so returning self: \\t\", self\n",
      "        return self\n",
      "        \n",
      "    '''\n",
      "    def get_all_leaf_2(self):\n",
      "        node_temp_queue = []\n",
      "        node_temp_queue.append(self)\n",
      "        leaf_list = []\n",
      "        while len(node_temp_queue) > 0:\n",
      "            node = node_temp_queue.pop(0)\n",
      "            if len(node.children) == 0:\n",
      "                leaf_list.append(node)\n",
      "            else:\n",
      "                for child in node.children:\n",
      "                    node_temp_queue.append(child)\n",
      "        return leaf_list\n",
      "    '''\n",
      "    \n",
      "    def get_path_from_all_leaf(self):\n",
      "        path_list = []\n",
      "        leaf_list = self.get_all_leaf()\n",
      "        for leaf in leaf_list:\n",
      "            path = []\n",
      "            path.append(leaf.value)\n",
      "            pointer_node = leaf\n",
      "            while pointer_node.parent != self:\n",
      "                path.append(pointer_node.parent.value)\n",
      "                pointer_node = pointer_node.parent\n",
      "            path.append(self.value)\n",
      "            path_list.append(path)\n",
      "        return path_list\n",
      "\n",
      "    \n",
      "    '''                \n",
      "    def get_path_from_leaves(self):\n",
      "        my_path = []\n",
      "        print \"self.value\", self.value\n",
      "        print \"len(self.children)\", len(self.children)\n",
      "        if self.children != []:\n",
      "            for a_child_node in self.children:\n",
      "                print \"a_child:\", a_child_node.value\n",
      "                print \"a_child:\", a_child_node.get_path()\n",
      "\n",
      "                my_path = my_path + a_child_node.get_path()\n",
      "        my_path.insert(0,self.value)\n",
      "        return my_path\n",
      "    '''"
     ],
