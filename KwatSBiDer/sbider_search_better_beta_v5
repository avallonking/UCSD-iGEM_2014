      "def sbider_path_memory_search(input_dictionary, activated_paths, start_operon, start_species):\n",
      "    \n",
      "    root_ope = node(start_operon) # root of operon tree\n",
      "    root_spe = node(start_species) # root of species tree\n",
      "\n",
      "    temp_queue_ope = [] # for growing ope_tree\n",
      "    temp_queue_ope.append(root_ope)\n",
      "    \n",
      "    temp_queue_spe = [] # for growing spe_tree\n",
      "    temp_queue_spe.append(root_spe)\n",
      "        \n",
      "    while len(activated_paths) > 0:\n",
      "        print \"$$$ sbider_path_memory_search $$$\"\n",
      "        print\n",
      "        print \"sbider_path_memory_search: activated_paths: \\t\", activated_paths\n",
      "        print\n",
      "        print \"sbider_path_memory_search: temp_queue_ope before pop: \\t\", temp_queue_ope\n",
      "        print \"sbider_path_memory_search: temp_queue_spe before pop: \\t\", temp_queue_spe\n",
      "        print\n",
      "        from_node = temp_queue_ope.pop(0)\n",
      "        from_operon = from_node.value\n",
      "        species_node = temp_queue_spe.pop(0)\n",
      "        species_value = species_node.value\n",
      "        print \"sbider_path_memory_search: from_operon: \\t\", from_operon\n",
      "        print \"sbider_path_memory_search: from_species: \\t\", species_value\n",
      "        print\n",
      "        print \"@@@ CALLING- sbider_path_memory @@@\"\n",
      "        children_operon, children_species = sbider_path_memory(input_dictionary, activated_paths, from_operon)\n",
      "        print \"@@@ -RETUNRED sbider_path_memory @@@\"\n",
      "        print\n",
      "        print \"sbider_path_memory_search: children_operon as returend: \\t\", children_operon\n",
      "        print \"sbider_path_memory_search: children_species as returend: \\t\", children_species\n",
      "        print\n",
      "        if not (len(children_operon) > 0 and len(children_species) > 0):\n",
      "            print \"sbider_path_memory_search: EMPTY children_operon or children_species.........\"\n",
      "            return [[],[]]\n",
      "        merged_children_operon = uniquely_merge_list_of_lists(children_operon)\n",
      "        merged_children_species = uniquely_merge_list_of_lists(children_species)\n",
      "        print \"sbider_path_memory_search: merged_children_operon: \\t\", merged_children_operon\n",
      "        print \"sbider_path_memory_search: merged_children_species: \\t\", merged_children_species\n",
      "        print\n",
      "        print\n",
      "        for a_merged_child_operon in merged_children_operon:\n",
      "            print \"sbider_path_memory_search: a_merged_child_operon: \\t\", a_merged_child_operon\n",
      "            a_child_node = node(a_merged_child_operon)\n",
      "            from_node.append_child(a_child_node)\n",
      "            temp_queue_ope.append(a_child_node)\n",
      "            print \"ROOT_OPE UPDATED \\n\", root_ope\n",
      "        print \"*\" * 99\n",
      "        print\n",
      "        for a_merged_child_species in merged_children_species:\n",
      "            print \"sbider_path_memory_search: a_merged_child_species: \\t\", a_merged_child_species\n",
      "            a_child_node = node(a_merged_child_species)\n",
      "            species_node.append_child(a_child_node)\n",
      "            temp_queue_spe.append(a_child_node)\n",
      "            print \"ROOT_SPE UPDATED \\n\", root_spe\n",
      "        print \"*\" * 99\n",
      "        \n",
      "    print \"sbider_path_memory_search: get_all_leaf\", root_ope.get_all_leaf()\n",
      "    print \"sbider_path_memory_search: get_all_leaf\", root_spe.get_all_leaf()\n",
      "    print \"sbider_path_memory_search: get_path_from_all_leaf\", root_ope.get_path_from_all_leaf()\n",
      "    print \"sbider_path_memory_search: get_path_from_all_leaf\", root_spe.get_path_from_all_leaf()\n",
      "    print\n",
      "    print \"RETURNING \\t\", [root_ope.get_path_from_all_leaf(), root_spe.get_path_from_all_leaf()]\n",
      "    return root_ope.get_path_from_all_leaf(), root_spe.get_path_from_all_leaf()"
     ],
     "language": "python",
     "metadata": {},
     "outputs": [],
